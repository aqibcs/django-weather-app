{
    "version": 2,
    "builds": [
        {
        "src": "weather_app/wsgi.py",
        "use": "@vercel/python"
        },
        {
        "src": "staticfiles/**",
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "weather_app/wsgi.py"
        }
    ]
}
